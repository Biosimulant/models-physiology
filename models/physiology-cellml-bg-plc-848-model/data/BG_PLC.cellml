<?xml version='1.0' encoding='UTF-8'?>
<!-- Bond graph script of PLC metabolism network, based on Bhalla and Iyengar 1999-->
<model name="BG_PLC_model" xmlns="http://www.cellml.org/cellml/1.1#" xmlns:cellml="http://www.cellml.org/cellml/1.1#" xmlns:xlink="http://www.w3.org/1999/xlink">
    <import xlink:href="units_and_constants/units_BG.cellml">
        <units name="mM" units_ref="mM"/>
        <units name="fmol" units_ref="fmol"/>
        <units name="per_fmol" units_ref="per_fmol"/>
        <units name="J_per_mol" units_ref="J_per_mol"/>
        <units name="J_per_K_per_mol" units_ref="J_per_K_per_mol"/>
        <units name="fmol_per_sec" units_ref="fmol_per_sec"/>
        <units name="C_per_mol" units_ref="C_per_mol"/>
        <units name="J_per_C" units_ref="J_per_C"/>
        <units name="fF" units_ref="fF"/>
        <units name="fC" units_ref="fC"/>
        <units name="fA" units_ref="fA"/>
        <units name="pL" units_ref="pL"/>
    </import>
    <import xlink:href="units_and_constants/constants_BG.cellml">
        <component component_ref="constants" name="constants"/>
    </import>
    <component name="environment">
        <variable name="time" public_interface="out" units="second"/>
        <variable initial_value="38" name="vol_myo" units="pL"/>
        <variable initial_value="0" name="q_DAG" public_interface="out" units="fmol"/>
        <variable initial_value="0" name="q_PC" public_interface="out" units="fmol"/>
        <variable initial_value="0" name="q_IP3" public_interface="out" units="fmol"/>
        <variable initial_value="0" name="q_Ino" public_interface="out" units="fmol"/>
        <variable initial_value="30.4" name="q_PLC" public_interface="out" units="fmol"/>
        <variable initial_value="38" name="q_Gq" public_interface="out" units="fmol"/>
        <variable initial_value="38" name="q_Ca" public_interface="out" units="fmol"/>
        <variable initial_value="0" name="q_Gq_PLC" public_interface="out" units="fmol"/>
        <variable initial_value="0" name="q_Ca_PLC" public_interface="out" units="fmol"/>
        <variable initial_value="38" name="q_Ca_Gq_PLC" public_interface="out" units="fmol"/>
        <variable initial_value="0" name="q_G_GDP" public_interface="out" units="fmol"/>
        <variable initial_value="380" name="q_PIP2" public_interface="out" units="fmol"/>
        <variable initial_value="0" name="q_CaPLC_PIP2" public_interface="out" units="fmol"/>
        <variable initial_value="0" name="q_CaGqPLC_PIP2" public_interface="out" units="fmol"/>
        <variable name="v_R0_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_R1_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_R2a_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_R2b_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_R3a_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_R3b_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_R4_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_R5_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_R6_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_R7_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_R8_PLC" public_interface="in" units="fmol_per_sec"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>q_DAG</ci>
                </apply>
                <apply>
                    <minus/>
                    <apply>
                        <plus/>
                        <ci>v_R2b_PLC</ci>
                        <ci>v_R3b_PLC</ci>
                    </apply>
                    <ci>v_R0_PLC</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>q_PC</ci>
                </apply>
                <ci>v_R0_PLC</ci>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>q_IP3</ci>
                </apply>
                <apply>
                    <minus/>
                    <apply>
                        <plus/>
                        <ci>v_R2b_PLC</ci>
                        <ci>v_R3b_PLC</ci>
                    </apply>
                    <ci>v_R1_PLC</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>q_Ino</ci>
                </apply>
                <ci>v_R1_PLC</ci>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>q_PLC</ci>
                </apply>
                <apply>
                    <minus/>
                    <apply>
                        <minus/>
                        <ci>v_R4_PLC</ci>
                    </apply>
                    <ci>v_R5_PLC</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>q_Gq</ci>
                </apply>
                <apply>
                    <plus/>
                    <apply>
                        <minus/>
                        <ci>v_R5_PLC</ci>
                    </apply>
                    <ci>v_R7_PLC</ci>
                    <ci>Gq_stim</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>q_Ca</ci>
                </apply>
                <apply>
                    <minus/>
                    <apply>
                        <minus/>
                        <ci>v_R4_PLC</ci>
                    </apply>
                    <ci>v_R6_PLC</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>q_Gq_PLC</ci>
                </apply>
                <apply>
                    <minus/>
                    <ci>v_R5_PLC</ci>
                    <ci>v_R6_PLC</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>q_Ca_PLC</ci>
                </apply>
                <apply>
                    <plus/>
                    <apply>
                        <minus/>
                        <ci>v_R2a_PLC</ci>
                    </apply>
                    <ci>v_R2b_PLC</ci>
                    <ci>v_R4_PLC</ci>
                    <ci>v_R7_PLC</ci>
                    <ci>v_R8_PLC</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>q_Ca_Gq_PLC</ci>
                </apply>
                <apply>
                    <minus/>
                    <apply>
                        <minus/>
                        <apply>
                            <plus/>
                            <apply>
                                <minus/>
                                <ci>v_R3a_PLC</ci>
                            </apply>
                            <ci>v_R3b_PLC</ci>
                            <ci>v_R6_PLC</ci>
                        </apply>
                        <ci>v_R7_PLC</ci>
                    </apply>
                    <ci>v_R8_PLC</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>q_G_GDP</ci>
                </apply>
                <ci>v_R8_PLC</ci>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>q_PIP2</ci>
                </apply>
                <apply>
                    <minus/>
                    <apply>
                        <minus/>
                        <ci>v_R2a_PLC</ci>
                    </apply>
                    <ci>v_R3a_PLC</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>q_CaPLC_PIP2</ci>
                </apply>
                <apply>
                    <minus/>
                    <ci>v_R2a_PLC</ci>
                    <ci>v_R2b_PLC</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>time</ci>
                    </bvar>
                    <ci>q_CaGqPLC_PIP2</ci>
                </apply>
                <apply>
                    <minus/>
                    <ci>v_R3a_PLC</ci>
                    <ci>v_R3b_PLC</ci>
                </apply>
            </apply>
        </math>
        <!-- Gq stim-->
        <variable name="Gq_stim" units="fmol_per_sec"/>
        <variable initial_value="1" name="stimPeriod" units="second"/>
        <variable initial_value="0.01" name="stimDuration" units="second"/>
        <variable name="tPeriod" units="second"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>tPeriod</ci>
                <apply>
                    <minus/>
                    <ci>time</ci>
                    <apply>
                        <times/>
                        <apply>
                            <floor/>
                            <apply>
                                <divide/>
                                <ci>time</ci>
                                <ci>stimPeriod</ci>
                            </apply>
                        </apply>
                        <ci>stimPeriod</ci>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>Gq_stim</ci>
                <piecewise>
                    <piece>
                        <apply>
                            <times/>
                            <cn cellml:units="dimensionless">0</cn>
                            <cn cellml:units="fmol_per_sec">100</cn>
                        </apply>
                        <apply>
                            <and/>
                            <apply>
                                <geq/>
                                <ci>tPeriod</ci>
                                <cn cellml:units="second">0.3</cn>
                            </apply>
                            <apply>
                                <leq/>
                                <ci>tPeriod</ci>
                                <apply>
                                    <plus/>
                                    <cn cellml:units="second">0.3</cn>
                                    <ci>stimDuration</ci>
                                </apply>
                            </apply>
                        </apply>
                    </piece>
                    <otherwise>
                        <cn cellml:units="fmol_per_sec">0.0</cn>
                    </otherwise>
                </piecewise>
            </apply>
        </math>
        <!-- conversion to mM-->
        <variable name="cGq" units="mM"/>
        <variable name="cCa" units="mM"/>
        <variable name="cDAG" units="mM"/>
        <variable name="cIP3" units="mM"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>cGq</ci>
                <apply>
                    <divide/>
                    <ci>q_Gq</ci>
                    <ci>vol_myo</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>cCa</ci>
                <apply>
                    <divide/>
                    <ci>q_Ca</ci>
                    <ci>vol_myo</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>cDAG</ci>
                <apply>
                    <divide/>
                    <ci>q_DAG</ci>
                    <ci>vol_myo</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>cIP3</ci>
                <apply>
                    <divide/>
                    <ci>q_IP3</ci>
                    <ci>vol_myo</ci>
                </apply>
            </apply>
        </math>
    </component>
    <component name="PLC_parameters">
        <!--        var kappa_R0_PLC: fmol_per_sec {init: 0.0475807, pub: out};
        var kappa_R1_PLC: fmol_per_sec {init: 0.121538, pub: out};
        var kappa_R2a_PLC: fmol_per_sec {init: 70395.1, pub: out};
        var kappa_R2b_PLC: fmol_per_sec {init: 2.70328, pub: out};
        var kappa_R3a_PLC: fmol_per_sec {init: 10988.5, pub: out};
        var kappa_R3b_PLC: fmol_per_sec {init: 0.00553812, pub: out};
        var kappa_R4_PLC: fmol_per_sec {init: 4.56488, pub: out};
        var kappa_R5_PLC: fmol_per_sec {init: 3.86201, pub: out};
        var kappa_R6_PLC: fmol_per_sec {init: 0.71257, pub: out};
        var kappa_R7_PLC: fmol_per_sec {init: 2.99279e-05, pub: out};
        var kappa_R8_PLC: fmol_per_sec {init: 0.00947718, pub: out};

        <variable initial_value="0.0700317" name="kappa_R0_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="0.178887" name="kappa_R1_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="71285.5" name="kappa_R2a_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="2.73747" name="kappa_R2b_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="5697.81" name="kappa_R3a_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="0.00287164" name="kappa_R3b_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="2.13383" name="kappa_R4_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="2.28508" name="kappa_R5_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="0.170556" name="kappa_R6_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="0.170556" name="kappa_R7_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="0.00226839" name="kappa_R8_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="0.577328" name="K_DAG" public_interface="out" units="per_fmol"/>
        <variable initial_value="0.00384885" name="K_PC" public_interface="out" units="per_fmol"/>
        <variable initial_value="3.76694" name="K_IP3" public_interface="out" units="per_fmol"/>
        <variable initial_value="0.00150678" name="K_Ino" public_interface="out" units="per_fmol"/>
        <variable initial_value="0.000942826" name="K_PLC" public_interface="out" units="per_fmol"/>
        <variable initial_value="0.000141635" name="K_Gq" public_interface="out" units="per_fmol"/>
        <variable initial_value="0.000180564" name="K_Ca" public_interface="out" units="per_fmol"/>
        <variable initial_value="0.117957" name="K_Gq_PLC" public_interface="out" units="per_fmol"/>
        <variable initial_value="0.126318" name="K_Ca_PLC" public_interface="out" units="per_fmol"/>
        <variable initial_value="1.58037" name="K_Ca_Gq_PLC" public_interface="out" units="per_fmol"/>
        <variable initial_value="0.000141635" name="K_G_GDP" public_interface="out" units="per_fmol"/>
        <variable initial_value="8.06835" name="K_PIP2" public_interface="out" units="per_fmol"/>
        <variable initial_value="18.9051" name="K_CaPLC_PIP2" public_interface="out" units="per_fmol"/>
        <variable initial_value="938.634" name="K_CaGqPLC_PIP2" public_interface="out" units="per_fmol"/>
    </component>
    <component name="PLC">
        <variable name="time" public_interface="in" units="second"/>
        <variable name="R" public_interface="in" units="J_per_K_per_mol"/>
        <variable name="T" public_interface="in" units="kelvin"/>
        <!-- Parameters-->
        <variable name="kappa_R0_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_R1_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_R2a_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_R2b_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_R3a_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_R3b_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_R4_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_R5_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_R6_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_R7_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_R8_PLC" public_interface="in" units="fmol_per_sec"/>
        <variable name="K_DAG" public_interface="in" units="per_fmol"/>
        <variable name="K_PC" public_interface="in" units="per_fmol"/>
        <variable name="K_IP3" public_interface="in" units="per_fmol"/>
        <variable name="K_Ino" public_interface="in" units="per_fmol"/>
        <variable name="K_PLC" public_interface="in" units="per_fmol"/>
        <variable name="K_Gq" public_interface="in" units="per_fmol"/>
        <variable name="K_Ca" public_interface="in" units="per_fmol"/>
        <variable name="K_Gq_PLC" public_interface="in" units="per_fmol"/>
        <variable name="K_Ca_PLC" public_interface="in" units="per_fmol"/>
        <variable name="K_Ca_Gq_PLC" public_interface="in" units="per_fmol"/>
        <variable name="K_G_GDP" public_interface="in" units="per_fmol"/>
        <variable name="K_PIP2" public_interface="in" units="per_fmol"/>
        <variable name="K_CaPLC_PIP2" public_interface="in" units="per_fmol"/>
        <variable name="K_CaGqPLC_PIP2" public_interface="in" units="per_fmol"/>
        <!-- state variables-->
        <variable name="q_DAG" public_interface="in" units="fmol"/>
        <variable name="q_PC" public_interface="in" units="fmol"/>
        <variable name="q_IP3" public_interface="in" units="fmol"/>
        <variable name="q_Ino" public_interface="in" units="fmol"/>
        <variable name="q_PLC" public_interface="in" units="fmol"/>
        <variable name="q_Gq" public_interface="in" units="fmol"/>
        <variable name="q_Ca" public_interface="in" units="fmol"/>
        <variable name="q_Gq_PLC" public_interface="in" units="fmol"/>
        <variable name="q_Ca_PLC" public_interface="in" units="fmol"/>
        <variable name="q_Ca_Gq_PLC" public_interface="in" units="fmol"/>
        <variable name="q_G_GDP" public_interface="in" units="fmol"/>
        <variable name="q_PIP2" public_interface="in" units="fmol"/>
        <variable name="q_CaPLC_PIP2" public_interface="in" units="fmol"/>
        <variable name="q_CaGqPLC_PIP2" public_interface="in" units="fmol"/>
        <!-- Constitutive parameters-->
        <variable name="mu_DAG" units="J_per_mol"/>
        <variable name="mu_PC" units="J_per_mol"/>
        <variable name="mu_IP3" units="J_per_mol"/>
        <variable name="mu_Ino" units="J_per_mol"/>
        <variable name="mu_PLC" units="J_per_mol"/>
        <variable name="mu_Gq" units="J_per_mol"/>
        <variable name="mu_Ca" units="J_per_mol"/>
        <variable name="mu_Gq_PLC" units="J_per_mol"/>
        <variable name="mu_Ca_PLC" units="J_per_mol"/>
        <variable name="mu_Ca_Gq_PLC" units="J_per_mol"/>
        <variable name="mu_G_GDP" units="J_per_mol"/>
        <variable name="mu_PIP2" units="J_per_mol"/>
        <variable name="mu_CaPLC_PIP2" units="J_per_mol"/>
        <variable name="mu_CaGqPLC_PIP2" units="J_per_mol"/>
        <variable name="v_R0_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable name="v_R1_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable name="v_R2a_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable name="v_R2b_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable name="v_R3a_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable name="v_R3b_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable name="v_R4_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable name="v_R5_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable name="v_R6_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable name="v_R7_PLC" public_interface="out" units="fmol_per_sec"/>
        <variable name="v_R8_PLC" public_interface="out" units="fmol_per_sec"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>mu_DAG</ci>
                <apply>
                    <times/>
                    <ci>R</ci>
                    <ci>T</ci>
                    <apply>
                        <ln/>
                        <apply>
                            <times/>
                            <ci>K_DAG</ci>
                            <ci>q_DAG</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>mu_PC</ci>
                <apply>
                    <times/>
                    <ci>R</ci>
                    <ci>T</ci>
                    <apply>
                        <ln/>
                        <apply>
                            <times/>
                            <ci>K_PC</ci>
                            <ci>q_PC</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>mu_IP3</ci>
                <apply>
                    <times/>
                    <ci>R</ci>
                    <ci>T</ci>
                    <apply>
                        <ln/>
                        <apply>
                            <times/>
                            <ci>K_IP3</ci>
                            <ci>q_IP3</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>mu_Ino</ci>
                <apply>
                    <times/>
                    <ci>R</ci>
                    <ci>T</ci>
                    <apply>
                        <ln/>
                        <apply>
                            <times/>
                            <ci>K_Ino</ci>
                            <ci>q_Ino</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>mu_PLC</ci>

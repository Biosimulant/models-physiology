        var K_PC: per_fmol {init: 0.00566494, pub: out};
        var K_IP3: per_fmol {init: 5.54437, pub: out};
        var K_Ino: per_fmol {init: 0.00221775, pub: out};
        var K_PLC: per_fmol {init: 0.00108946, pub: out};

  <component name="x_010">
    <variable cmeta:id="x_010.V3" name="V3" public_interface="in" units="flux" />
    <variable cmeta:id="x_010.x_010" name="x_010" public_interface="out" units="micromolar" />
    <variable cmeta:id="x_010.V2" name="V2" public_interface="in" units="flux" />
    <variable cmeta:id="x_010.time" name="time" public_interface="in" units="second" />
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="9" cmeta:id="9">
      <apply id="x_010_diff_eq">
        <eq />
        <apply>
          <diff />
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>x_010</ci>
        </apply>
        <apply>
          <minus />
          <ci>V3</ci>
          <ci>V2</ci>
        </apply>
      </apply>
    </math>
  </component>
  <component name="Ca_i">
    <variable cmeta:id="Ca_i.v2" name="v2" units="first_order_rate_constant" />
    <variable cmeta:id="Ca_i.v1" name="v1" units="first_order_rate_constant" />
    <variable cmeta:id="Ca_i.Ca_i" name="Ca_i" public_interface="out" units="micromolar" />
    <variable cmeta:id="Ca_i.time" name="time" public_interface="in" units="second" />
    <variable cmeta:id="Ca_i.c1" name="c1" public_interface="in" units="dimensionless" />
    <variable cmeta:id="Ca_i.v3" name="v3" units="second_order_rate_constant" />
    <variable cmeta:id="Ca_i.J2" name="J2" units="flux" />
    <variable cmeta:id="Ca_i.k3" name="k3" units="micromolar" />
    <variable cmeta:id="Ca_i.J1" name="J1" units="flux" />
    <variable cmeta:id="Ca_i.Ca_ER" name="Ca_ER" public_interface="in" units="micromolar" />
    <variable cmeta:id="Ca_i.P_open" name="P_open" public_interface="in" units="dimensionless" />
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="1" cmeta:id="1">
      <apply>
        <eq />
        <apply>
          <diff />
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>Ca_i</ci>
        </apply>
        <apply>
          <minus />
          <ci>J1</ci>
          <ci>J2</ci>
        </apply>
      </apply>
      <apply id="J1_calculation">
        <eq />
        <ci>J1</ci>
        <apply>
          <times />
          <ci>c1</ci>
          <apply>
            <plus />
            <apply>
              <times />
              <ci>v1</ci>
              <ci>P_open</ci>
            </apply>
            <ci>v2</ci>
          </apply>
          <apply>
            <minus />
            <ci>Ca_ER</ci>
            <ci>Ca_i</ci>
          </apply>
        </apply>
      </apply>
      <apply id="J2_calculation">
        <eq />
        <ci>J2</ci>
        <apply>
          <divide />
          <apply>
            <times />
            <ci>v3</ci>
            <apply>
              <power />
              <ci>Ca_i</ci>
              <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2.0</cn>
            </apply>
          </apply>
          <apply>
            <plus />
            <apply>
              <power />
              <ci>Ca_i</ci>
              <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2.0</cn>
            </apply>
            <apply>
              <power />
              <ci>k3</ci>
              <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">2.0</cn>
            </apply>
          </apply>
        </apply>
      </apply>
    </math>
  </component>

        </apply>
      </apply>
    </math>
  </component>
  <component name="x_011">
    <variable cmeta:id="x_011.x_010" name="x_010" public_interface="in" units="micromolar" />
    <variable cmeta:id="x_011.x_000" name="x_000" public_interface="in" units="micromolar" />
    <variable cmeta:id="x_011.x_011" name="x_011" public_interface="out" units="micromolar" />
    <variable cmeta:id="x_011.x_001" name="x_001" public_interface="in" units="micromolar" />
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="8a" cmeta:id="8a">
      <apply id="x_011_eq">
        <eq />
        <ci>x_011</ci>
        <apply>
          <minus />
          <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1.0</cn>
          <apply>
            <plus />
            <ci>x_000</ci>
            <ci>x_001</ci>
            <ci>x_010</ci>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
  <component name="receptor_dissociation_constants">
    <variable cmeta:id="receptor_dissociation_constants.a5" name="a5" public_interface="in" units="second_order_rate_constant" />
    <variable cmeta:id="receptor_dissociation_constants.a4" name="a4" public_interface="in" units="second_order_rate_constant" />
    <variable cmeta:id="receptor_dissociation_constants.b5" name="b5" public_interface="in" units="first_order_rate_constant" />
    <variable cmeta:id="receptor_dissociation_constants.IP3_cold" name="IP3_cold" units="nanomolar" />
    <variable cmeta:id="receptor_dissociation_constants.a3" name="a3" public_interface="in" units="second_order_rate_constant" />
    <variable cmeta:id="receptor_dissociation_constants.b4" name="b4" public_interface="in" units="first_order_rate_constant" />
    <variable cmeta:id="receptor_dissociation_constants.a2" name="a2" public_interface="in" units="second_order_rate_constant" />
    <variable cmeta:id="receptor_dissociation_constants.b3" name="b3" public_interface="in" units="first_order_rate_constant" />
    <variable cmeta:id="receptor_dissociation_constants.d5" name="d5" public_interface="out" units="nanomolar" />
    <variable cmeta:id="receptor_dissociation_constants.b2" name="b2" public_interface="in" units="first_order_rate_constant" />
    <variable cmeta:id="receptor_dissociation_constants.d4" name="d4" public_interface="out" units="nanomolar" />
    <variable cmeta:id="receptor_dissociation_constants.d3" name="d3" public_interface="out" units="nanomolar" />
    <variable cmeta:id="receptor_dissociation_constants.K_d1" name="K_d1" units="nanomolar" />
    <variable cmeta:id="receptor_dissociation_constants.d2" name="d2" public_interface="out" units="micromolar" />
    <variable cmeta:id="receptor_dissociation_constants.K_d2" name="K_d2" units="nanomolar" />
    <variable cmeta:id="receptor_dissociation_constants.d1" name="d1" public_interface="out" units="micromolar" />
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="5" cmeta:id="5">
      <apply id="d1_calculation">
        <eq />
        <ci>d1</ci>
        <apply>
          <minus />
          <ci>K_d1</ci>
          <ci>IP3_cold</ci>
        </apply>
      </apply>
    </math>
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="5A">
      <apply id="d2_calculation">
        <eq />
        <ci>d2</ci>
        <apply>
          <divide />
          <ci>b2</ci>
          <ci>a2</ci>
        </apply>
      </apply>
    </math>
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="5B">
      <apply id="d3_calculation">
        <eq />
        <ci>d3</ci>
        <apply>
          <minus />
          <apply>
            <times />
            <apply>
              <minus />
              <ci>K_d2</ci>
              <ci>IP3_cold</ci>
            </apply>
            <apply>
              <plus />
              <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1</cn>
              <ci>d2</ci>
            </apply>
          </apply>
          <apply>
            <times />
            <ci>d1</ci>
            <ci>d2</ci>
          </apply>
        </apply>
      </apply>
    </math>
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="5C">
      <apply id="d4_calculation">
        <eq />
        <ci>d4</ci>
        <apply>
          <divide />
          <apply>
            <times />
            <ci>d1</ci>
            <ci>d2</ci>
          </apply>
          <ci>d3</ci>
        </apply>
      </apply>
    </math>
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="5D">
      <apply id="d5_calculation">
        <eq />
        <ci>d5</ci>
        <apply>
          <divide />
          <ci>b5</ci>
          <ci>a5</ci>
        </apply>
      </apply>
    </math>
  </component>
  <component name="IP3">
    <variable cmeta:id="IP3.Ca_i" name="Ca_i" public_interface="in" units="micromolar" />
    <variable cmeta:id="IP3.IP3" name="IP3" public_interface="out" units="micromolar" />
    <variable cmeta:id="IP3.v4" name="v4" units="first_order_rate_constant" />
    <variable cmeta:id="IP3.alpha" name="alpha" units="dimensionless" />
    <variable cmeta:id="IP3.Ir" name="Ir" units="flux" />
    <variable cmeta:id="IP3.k4" name="k4" units="micromolar" />
    <variable cmeta:id="IP3.time" name="time" public_interface="in" units="second" />
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="3" cmeta:id="3">
      <apply>
        <eq />
        <apply>
          <diff />
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>IP3</ci>
        </apply>
        <apply>
          <minus />
          <apply>
            <times />
            <ci>v4</ci>
            <apply>
              <divide />
              <apply>
                <plus />
                <ci>Ca_i</ci>
                <apply>
                  <times />
                  <apply>
                    <minus />
                    <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">1.0</cn>
                    <ci>alpha</ci>
                  </apply>
                  <ci>k4</ci>
                </apply>
              </apply>
              <apply>
                <plus />
                <ci>Ca_i</ci>
                <ci>k4</ci>
              </apply>
            </apply>
          </apply>
          <apply>
            <times />
            <ci>Ir</ci>
            <ci>IP3</ci>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
  <component name="environment">
    <variable cmeta:id="environment.time" name="time" public_interface="out" units="second" />
  </component>
  <component name="fluxes">
    <variable cmeta:id="fluxes.V1" name="V1" public_interface="out" units="flux" />
    <variable cmeta:id="fluxes.V3" name="V3" public_interface="out" units="flux" />
    <variable cmeta:id="fluxes.V2" name="V2" public_interface="out" units="flux" />
    <variable cmeta:id="fluxes.V4" name="V4" public_interface="out" units="flux" />
    <variable cmeta:id="fluxes.d5" name="d5" public_interface="in" units="nanomolar" />
    <variable cmeta:id="fluxes.d4" name="d4" public_interface="in" units="nanomolar" />
    <variable cmeta:id="fluxes.a4" name="a4" public_interface="in" units="second_order_rate_constant" />
    <variable cmeta:id="fluxes.a5" name="a5" public_interface="in" units="second_order_rate_constant" />
    <variable cmeta:id="fluxes.x_001" name="x_001" public_interface="in" units="micromolar" />
    <variable cmeta:id="fluxes.x_000" name="x_000" public_interface="in" units="micromolar" />
    <variable cmeta:id="fluxes.x_011" name="x_011" public_interface="in" units="micromolar" />
    <variable cmeta:id="fluxes.Ca_i" name="Ca_i" public_interface="in" units="micromolar" />
    <variable cmeta:id="fluxes.x_010" name="x_010" public_interface="in" units="micromolar" />
    <variable cmeta:id="fluxes.time" name="time" public_interface="in" units="second" />
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="10" cmeta:id="10">
      <apply id="V1_calculation">
        <eq />
        <ci>V1</ci>
        <apply>
          <times />
          <ci>a4</ci>
          <apply>
            <minus />
            <apply>
              <times />
              <ci>Ca_i</ci>
              <ci>x_000</ci>
            </apply>
            <apply>
              <times />
              <ci>d4</ci>
              <ci>x_001</ci>
            </apply>
          </apply>
        </apply>
      </apply>
      <apply id="V2_calculation">
        <eq />
        <ci>V2</ci>
        <apply>
          <times />
          <ci>a4</ci>
          <apply>
            <minus />
            <apply>
              <times />
              <ci>Ca_i</ci>
              <ci>x_010</ci>
            </apply>
            <apply>
              <times />
              <ci>d4</ci>
              <ci>x_011</ci>
            </apply>
          </apply>
        </apply>
      </apply>
      <apply id="V3_calculation">
        <eq />
        <ci>V3</ci>
        <apply>
          <times />
          <ci>a5</ci>
          <apply>
            <minus />
            <apply>
              <times />
              <ci>Ca_i</ci>
              <ci>x_000</ci>
            </apply>
            <apply>
              <times />
              <ci>d5</ci>
              <ci>x_010</ci>
            </apply>
          </apply>
        </apply>
      </apply>
      <apply id="V4_calculation">
        <eq />
        <ci>V4</ci>
        <apply>
          <times />
          <ci>a5</ci>
          <apply>
            <minus />
            <apply>
              <times />
              <ci>Ca_i</ci>
              <ci>x_001</ci>
            </apply>
            <apply>
              <times />
              <ci>d5</ci>
              <ci>x_011</ci>
            </apply>
          </apply>
        </apply>
      </apply>
    </math>
  </component>
  <component name="probability_of_an_open_IP3_receptor_channel">
    <variable cmeta:id="probability_of_an_open_IP3_receptor_channel.d2" name="d2" public_interface="in" units="micromolar" />
    <variable cmeta:id="probability_of_an_open_IP3_receptor_channel.Ca_i" name="Ca_i" public_interface="in" units="micromolar" />
    <variable cmeta:id="probability_of_an_open_IP3_receptor_channel.IP3" name="IP3" public_interface="in" units="micromolar" />
    <variable cmeta:id="probability_of_an_open_IP3_receptor_channel.d1" name="d1" public_interface="in" units="micromolar" />
    <variable cmeta:id="probability_of_an_open_IP3_receptor_channel.P_open" name="P_open" public_interface="out" units="dimensionless" />
    <variable cmeta:id="probability_of_an_open_IP3_receptor_channel.d5" name="d5" public_interface="in" units="nanomolar" />
    <variable cmeta:id="probability_of_an_open_IP3_receptor_channel.d3" name="d3" public_interface="in" units="nanomolar" />
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="6" cmeta:id="6">
      <apply id="P_open_calculation">
        <eq />
        <ci>P_open</ci>
        <apply>
          <power />
          <apply>
            <divide />
            <apply>
              <times />
              <ci>Ca_i</ci>
              <ci>IP3</ci>
              <ci>d2</ci>
            </apply>
            <apply>
              <times />
              <apply>
                <plus />
                <apply>
                  <times />
                  <ci>Ca_i</ci>
                  <ci>IP3</ci>
                </apply>
                <apply>
                  <times />
                  <ci>IP3</ci>
                  <ci>d2</ci>
                </apply>
                <apply>
                  <times />
                  <ci>d1</ci>
                  <ci>d2</ci>
                </apply>
                <apply>
                  <times />
                  <ci>Ca_i</ci>
                  <ci>d3</ci>
                </apply>
              </apply>
              <apply>
                <plus />
                <ci>Ca_i</ci>
                <ci>d5</ci>
              </apply>
            </apply>
          </apply>
          <cn xmlns:cellml="http://www.cellml.org/cellml/1.0#" cellml:units="dimensionless">3.0</cn>
        </apply>
      </apply>
    </math>
  </component>
  <component name="Ca_ER">
    <variable cmeta:id="Ca_ER.c1" name="c1" public_interface="in" units="dimensionless" />
    <variable cmeta:id="Ca_ER.Ca_ER" name="Ca_ER" public_interface="out" units="micromolar" />

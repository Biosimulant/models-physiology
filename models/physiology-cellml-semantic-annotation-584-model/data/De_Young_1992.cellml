    <variable cmeta:id="x_000.x_000" name="x_000" public_interface="out" units="micromolar" />
    <variable cmeta:id="x_000.V3" name="V3" public_interface="in" units="flux" />
    <variable cmeta:id="x_000.V1" name="V1" public_interface="in" units="flux" />
    <math xmlns="http://www.w3.org/1998/Math/MathML" id="7" cmeta:id="7">

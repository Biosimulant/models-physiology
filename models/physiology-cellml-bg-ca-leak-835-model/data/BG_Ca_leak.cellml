<?xml version='1.0' encoding='UTF-8'?>
<!--Simple mass action reaction for this flux-->
<model name="Ca_leak_model" xmlns="http://www.cellml.org/cellml/1.1#" xmlns:cellml="http://www.cellml.org/cellml/1.1#" xmlns:xlink="http://www.w3.org/1999/xlink">
    <import xlink:href="units_and_constants/units_BG.cellml">
        <units name="mM" units_ref="mM"/>
        <units name="per_mol" units_ref="per_mol"/>
        <units name="J_per_mol" units_ref="J_per_mol"/>
        <units name="J_per_K_per_mol" units_ref="J_per_K_per_mol"/>
        <units name="J_per_C" units_ref="J_per_C"/>
        <units name="mol_per_s" units_ref="mol_per_s"/>
        <units name="C_per_mol" units_ref="C_per_mol"/>
        <units name="microm3" units_ref="microm3"/>
        <units name="fF" units_ref="fF"/>
        <units name="fC" units_ref="fC"/>
        <units name="fA" units_ref="fA"/>
        <units name="fmol" units_ref="fmol"/>
        <units name="fmol_per_sec" units_ref="fmol_per_sec"/>
        <units name="per_fmol" units_ref="per_fmol"/>
        <units name="pL" units_ref="pL"/>
        <units name="mA_per_mM" units_ref="mA_per_mM"/>
    </import>
    <component name="environment">
        <variable name="t" public_interface="out" units="second"/>
        <variable initial_value="25.8" name="w_i" public_interface="out" units="pL"/>
        <variable name="w_SR" public_interface="out" units="pL"/>
        <variable initial_value="8.31" name="R" public_interface="out" units="J_per_K_per_mol"/>
        <variable initial_value="310" name="T" public_interface="out" units="kelvin"/>
        <variable name="v_leak" public_interface="in" units="fmol_per_sec"/>
        <variable initial_value="0.00456" name="q_Ca_i" public_interface="out" units="fmol"/>
        <variable initial_value="6.41e-1" name="q_Ca_SR" public_interface="out" units="fmol"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>w_SR</ci>
                <apply>
                    <times/>
                    <ci>w_i</ci>
                    <cn cellml:units="dimensionless">0.088235</cn>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>q_Ca_SR</ci>
                </apply>
                <apply>
                    <minus/>
                    <ci>v_leak</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>q_Ca_i</ci>
                </apply>
                <ci>v_leak</ci>
            </apply>
        </math>
    </component>
    <component name="Ca_leak_parameters">
        <variable initial_value="0.698143" name="kappa_leak" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="0.367039" name="K_Ca_SR" public_interface="out" units="per_fmol"/>
        <variable initial_value="0.0323856" name="K_Ca_i" public_interface="out" units="per_fmol"/>
    </component>
    <component name="Ca_leak">
        <variable name="t" public_interface="in" units="second"/>
        <variable name="R" public_interface="in" units="J_per_K_per_mol"/>
        <variable name="T" public_interface="in" units="kelvin"/>
        <variable name="K_Ca_SR" public_interface="in" units="per_fmol"/>
        <variable name="K_Ca_i" public_interface="in" units="per_fmol"/>
        <variable name="kappa_leak" public_interface="in" units="fmol_per_sec"/>
        <variable name="q_Ca_SR" public_interface="in" units="fmol"/>
        <variable name="q_Ca_i" public_interface="in" units="fmol"/>
        <variable name="mu_Ca_SR" units="J_per_mol"/>
        <variable name="mu_Ca_i" units="J_per_mol"/>
        <variable name="v_leak" public_interface="out" units="fmol_per_sec"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>mu_Ca_SR</ci>
                <apply>
                    <times/>
                    <apply>
                        <times/>
                        <ci>R</ci>
                        <ci>T</ci>
                    </apply>
                    <apply>
                        <ln/>
                        <apply>
                            <times/>
                            <ci>K_Ca_SR</ci>
                            <ci>q_Ca_SR</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>mu_Ca_i</ci>
                <apply>
                    <times/>
                    <apply>
                        <times/>
                        <ci>R</ci>
                        <ci>T</ci>
                    </apply>
                    <apply>
                        <ln/>
                        <apply>
                            <times/>
                            <ci>K_Ca_i</ci>
                            <ci>q_Ca_i</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>v_leak</ci>
                <apply>
                    <times/>
                    <ci>kappa_leak</ci>
                    <apply>
                        <minus/>
                        <apply>
                            <exp/>
                            <apply>
                                <divide/>
                                <ci>mu_Ca_SR</ci>
                                <apply>
                                    <times/>
                                    <ci>R</ci>
                                    <ci>T</ci>
                                </apply>
                            </apply>
                        </apply>
                        <apply>
                            <exp/>
                            <apply>
                                <divide/>
                                <ci>mu_Ca_i</ci>
                                <apply>
                                    <times/>
                                    <ci>R</ci>
                                    <ci>T</ci>
                                </apply>
                            </apply>
                        </apply>
                    </apply>
                </apply>
            </apply>
        </math>
    </component>
    <connection>
        <map_components component_1="environment" component_2="Ca_leak"/>
        <map_variables variable_1="t" variable_2="t"/>
        <map_variables variable_1="R" variable_2="R"/>
        <map_variables variable_1="T" variable_2="T"/>
        <map_variables variable_1="v_leak" variable_2="v_leak"/>
        <map_variables variable_1="q_Ca_SR" variable_2="q_Ca_SR"/>
        <map_variables variable_1="q_Ca_i" variable_2="q_Ca_i"/>
    </connection>
    <connection>
        <map_components component_1="Ca_leak" component_2="Ca_leak_parameters"/>
        <map_variables variable_1="K_Ca_SR" variable_2="K_Ca_SR"/>
        <map_variables variable_1="K_Ca_i" variable_2="K_Ca_i"/>
        <map_variables variable_1="kappa_leak" variable_2="kappa_leak"/>
    </connection>
    <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
        <rdf:Description xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" rdf:about="#model">
            <bqs:reference xmlns:bqs="http://www.cellml.org/bqs/1.0#" rdf:parseType="Resource" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
                <dc:subject xmlns:dc="http://purl.org/dc/elements/1.1/" rdf:parseType="Resource" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
                    <bqs:subject_type xmlns:bqs="http://www.cellml.org/bqs/1.0#">keyword</bqs:subject_type>
                    <rdf:value xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
                        <rdf:Bag xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
                            <rdf:li xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">protein-modules</rdf:li>
                        </rdf:Bag>
                    </rdf:value>
                </dc:subject>
            </bqs:reference>
        </rdf:Description>
    </rdf:RDF>
</model>

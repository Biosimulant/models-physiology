    <component name="SERCA">
        <!-- Physical parameters-->
        <variable name="t" public_interface="in" units="second"/>
        <variable name="R" public_interface="in" units="J_per_K_per_mol"/>
        <variable name="T" public_interface="in" units="kelvin"/>
        <variable name="F" public_interface="in" units="C_per_mol"/>
        <!-- Bond graph parameters-->
        <variable name="K_P1_SERCA" public_interface="in" units="per_fmol"/>
        <variable name="K_P10_SERCA" public_interface="in" units="per_fmol"/>
        <variable name="K_P2_SERCA" public_interface="in" units="per_fmol"/>
        <variable name="K_P2a_SERCA" public_interface="in" units="per_fmol"/>
        <variable name="K_P4_SERCA" public_interface="in" units="per_fmol"/>
        <variable name="K_P5_SERCA" public_interface="in" units="per_fmol"/>
        <variable name="K_P6_SERCA" public_interface="in" units="per_fmol"/>
        <variable name="K_P8_SERCA" public_interface="in" units="per_fmol"/>
        <variable name="K_P9_SERCA" public_interface="in" units="per_fmol"/>
        <variable name="K_Ca_i_SERCA" public_interface="in" units="per_fmol"/>
        <variable name="K_Ca_SR" public_interface="in" units="per_fmol"/>
        <variable name="K_H" public_interface="in" units="per_fmol"/>
        <variable name="K_MgADP" public_interface="in" units="per_fmol"/>
        <variable name="K_MgATP" public_interface="in" units="per_fmol"/>
        <variable name="K_Pi" public_interface="in" units="per_fmol"/>
        <variable name="kappa_SERCA_R10_1" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_SERCA_R1_2" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_SERCA_R2_2a" public_interface="in" units="fmol_per_sec"/>

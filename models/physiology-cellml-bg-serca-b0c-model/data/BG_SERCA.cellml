        <variable name="Af_R9_10" units="J_per_mol"/>
        <variable name="Ar_R9_10" units="J_per_mol"/>
        <variable name="v_SERCA_R9_10" units="fmol_per_sec"/>
        <variable name="Af_R10_1" units="J_per_mol"/>
        <variable name="Ar_R10_1" units="J_per_mol"/>
        <variable name="v_SERCA_R10_1" units="fmol_per_sec"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>mu_Ca_i</ci>
                <apply>
                    <times/>
                    <ci>R</ci>
                    <ci>T</ci>
                    <apply>
                        <ln/>
                        <apply>
                            <times/>
                            <ci>K_Ca_i_SERCA</ci>
                            <ci>q_Ca_i</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>mu_Ca_SR</ci>

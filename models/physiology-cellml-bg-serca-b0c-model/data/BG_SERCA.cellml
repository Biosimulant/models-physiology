        <variable initial_value="993148.433" name="K_P6_SERCA" public_interface="out" units="per_fmol"/>
        <variable initial_value="37.7379" name="K_P8_SERCA" public_interface="out" units="per_fmol"/>
        <variable initial_value="2230.2717" name="K_P9_SERCA" public_interface="out" units="per_fmol"/>
        <variable initial_value="1.9058" name="K_Ca_i_SERCA" public_interface="out" units="per_fmol"/>
        <variable initial_value="31.764" name="K_Ca_SR" public_interface="out" units="per_fmol"/>
        <variable initial_value="1862.5406" name="K_H" public_interface="out" units="per_fmol"/>
        <variable initial_value="5.8126e-07" name="K_MgADP" public_interface="out" units="per_fmol"/>
        <variable initial_value="244.3021" name="K_MgATP" public_interface="out" units="per_fmol"/>
        <variable initial_value="0.014921" name="K_Pi" public_interface="out" units="per_fmol"/>
        <variable initial_value="0.049926" name="kappa_SERCA_R10_1" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="0.00053004" name="kappa_SERCA_R1_2" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="8326784.0537" name="kappa_SERCA_R2_2a" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="1567.7476" name="kappa_SERCA_R2_4" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="1567.7476" name="kappa_SERCA_R4_5" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="3063.4006" name="kappa_SERCA_R5_6" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="130852.3839" name="kappa_SERCA_R6_8" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="11612934.8748" name="kappa_SERCA_R8_9" public_interface="out" units="fmol_per_sec"/>
        <variable initial_value="11612934.8748" name="kappa_SERCA_R9_10" public_interface="out" units="fmol_per_sec"/>

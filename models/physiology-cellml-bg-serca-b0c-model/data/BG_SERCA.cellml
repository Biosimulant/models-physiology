        <variable name="kappa_SERCA_R4_5" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_SERCA_R5_6" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_SERCA_R6_8" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_SERCA_R8_9" public_interface="in" units="fmol_per_sec"/>
        <variable name="kappa_SERCA_R9_10" public_interface="in" units="fmol_per_sec"/>
        <variable initial_value="2" name="n_Ca_i" units="dimensionless"/>
        <variable initial_value="2" name="n_Ca_SR" units="dimensionless"/>
        <variable initial_value="2" name="n_H" units="dimensionless"/>
        <!-- Input from global environment-->
        <variable name="q_Ca_i" public_interface="in" units="fmol"/>
        <variable name="q_Ca_SR" public_interface="in" units="fmol"/>
        <variable name="q_H" public_interface="in" units="fmol"/>
        <variable name="q_Pi" public_interface="in" units="fmol"/>
        <variable name="q_MgADP" public_interface="in" units="fmol"/>
        <variable name="q_MgATP" public_interface="in" units="fmol"/>
        <!-- gate        -->
        <variable name="q_P1_SERCA" public_interface="in" units="fmol"/>
        <variable name="q_P2_SERCA" public_interface="in" units="fmol"/>
        <variable name="q_P2a_SERCA" public_interface="in" units="fmol"/>
        <variable name="q_P4_SERCA" public_interface="in" units="fmol"/>
        <variable name="q_P5_SERCA" public_interface="in" units="fmol"/>
        <variable name="q_P10_SERCA" public_interface="in" units="fmol"/>
        <variable name="q_P6_SERCA" public_interface="in" units="fmol"/>
        <variable name="q_P8_SERCA" public_interface="in" units="fmol"/>
        <variable name="q_P9_SERCA" public_interface="in" units="fmol"/>
        <variable name="q_SERCA_Ca_complexes" public_interface="out" units="fmol"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>q_SERCA_Ca_complexes</ci>
                <apply>
                    <plus/>
                    <apply>
                        <times/>
                        <ci>n_Ca_i</ci>
                        <ci>q_P4_SERCA</ci>
                    </apply>
                    <apply>
                        <times/>
                        <ci>n_Ca_i</ci>
                        <ci>q_P5_SERCA</ci>
                    </apply>
                    <apply>
                        <times/>
                        <ci>n_Ca_SR</ci>
                        <ci>q_P6_SERCA</ci>
                    </apply>
                </apply>
            </apply>
        </math>
        <!-- Concentrations-->
        <variable name="c_Ca_i" units="mM"/>
        <variable name="c_Ca_SR" units="mM"/>
        <variable name="c_H" units="mM"/>
        <variable name="c_MgADP" units="mM"/>
        <variable name="c_MgATP" units="mM"/>
        <variable name="c_Pi" units="mM"/>
        <variable initial_value="34.0" name="vol_i" units="pL"/>
        <variable name="vol_sr" units="pL"/>
        <variable name="vol_isr" units="pL"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>vol_sr</ci>
                <apply>
                    <times/>
                    <ci>vol_i</ci>
                    <cn cellml:units="dimensionless">0.035</cn>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>vol_isr</ci>
                <apply>
                    <plus/>
                    <ci>vol_i</ci>
                    <ci>vol_sr</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>c_Ca_i</ci>
                <apply>
                    <divide/>
                    <ci>q_Ca_i</ci>
                    <ci>vol_i</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>c_Ca_SR</ci>
                <apply>
                    <divide/>
                    <ci>q_Ca_SR</ci>
                    <ci>vol_sr</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>c_H</ci>
                <apply>
                    <divide/>
                    <ci>q_H</ci>
                    <ci>vol_isr</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>c_MgADP</ci>
                <apply>
                    <divide/>
                    <ci>q_MgADP</ci>
                    <ci>vol_i</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>c_MgATP</ci>
                <apply>
                    <divide/>
                    <ci>q_MgATP</ci>
                    <ci>vol_i</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>c_Pi</ci>
                <apply>
                    <divide/>
                    <ci>q_Pi</ci>
                    <ci>vol_i</ci>
                </apply>
            </apply>
        </math>
        <!-- Constitutive equations-->
        <variable name="mu_Ca_i" units="J_per_mol"/>
        <variable name="v_Ca_i_SERCA" public_interface="out" units="fmol_per_sec"/>
        <variable name="mu_Ca_SR" units="J_per_mol"/>
        <variable name="v_Ca_SR_SERCA" public_interface="out" units="fmol_per_sec"/>

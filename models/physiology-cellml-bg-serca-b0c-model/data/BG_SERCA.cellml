<?xml version='1.0'?>
<!-- SERCA model adapted from Pan et al https://arxiv.org/abs/1806.04341-->
<model name="BG_SERCA" xmlns="http://www.cellml.org/cellml/1.1#" xmlns:cellml="http://www.cellml.org/cellml/1.1#" xmlns:xlink="http://www.w3.org/1999/xlink">
    <import xlink:href="units_and_constants/units_BG.cellml">
        <units name="mM" units_ref="mM"/>
        <units name="fmol" units_ref="fmol"/>
        <units name="per_fmol" units_ref="per_fmol"/>
        <units name="J_per_mol" units_ref="J_per_mol"/>
        <units name="J_per_K_per_mol" units_ref="J_per_K_per_mol"/>
        <units name="fmol_per_sec" units_ref="fmol_per_sec"/>
        <units name="C_per_mol" units_ref="C_per_mol"/>
        <units name="pL" units_ref="pL"/>
    </import>
    <import xlink:href="units_and_constants/constants_BG.cellml">
        <component component_ref="constants" name="constants"/>
    </import>
    <component name="environment">
        <variable name="t" public_interface="out" units="second"/>
        <!-- init-->
        <variable initial_value="0.0057" name="q_Ca_i" public_interface="out" units="fmol"/>
        <variable initial_value="2.28" name="q_Ca_SR" public_interface="out" units="fmol"/>
        <variable initial_value="0.004028" name="q_H" public_interface="out" units="fmol"/>
        <variable initial_value="570" name="q_Pi" public_interface="out" units="fmol"/>
        <variable initial_value="1.3794" name="q_MgADP" public_interface="out" units="fmol"/>
        <variable initial_value="3.8" name="q_MgATP" public_interface="out" units="fmol"/>
        <variable initial_value="0.11111" name="q_P1_SERCA" public_interface="out" units="fmol"/>
        <variable initial_value="0.11111" name="q_P2_SERCA" public_interface="out" units="fmol"/>
        <variable initial_value="0.11111" name="q_P2a_SERCA" public_interface="out" units="fmol"/>
        <variable initial_value="0.11111" name="q_P4_SERCA" public_interface="out" units="fmol"/>
        <variable initial_value="0.11111" name="q_P5_SERCA" public_interface="out" units="fmol"/>
        <variable initial_value="0.11111" name="q_P10_SERCA" public_interface="out" units="fmol"/>
        <variable initial_value="0.11111" name="q_P6_SERCA" public_interface="out" units="fmol"/>
        <variable initial_value="0.11111" name="q_P8_SERCA" public_interface="out" units="fmol"/>
        <variable initial_value="0.11111" name="q_P9_SERCA" public_interface="out" units="fmol"/>
        <!-- SERCA imports-->
        <variable name="v_Ca_i_SERCA" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_Ca_SR_SERCA" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_H_SERCA" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_MgADP_SERCA" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_MgATP_SERCA" public_interface="in" units="fmol_per_sec"/>
        <variable name="v_Pi_SERCA" public_interface="in" units="fmol_per_sec"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>q_Ca_i</ci>
                </apply>
                <ci>v_Ca_i_SERCA</ci>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>q_Ca_SR</ci>
                </apply>
                <ci>v_Ca_SR_SERCA</ci>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>q_H</ci>
                </apply>
                <ci>v_H_SERCA</ci>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>q_MgADP</ci>
                </apply>
                <ci>v_MgADP_SERCA</ci>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>q_MgATP</ci>
                </apply>
                <ci>v_MgATP_SERCA</ci>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>q_Pi</ci>
                </apply>
                <ci>v_Pi_SERCA</ci>
            </apply>
        </math>

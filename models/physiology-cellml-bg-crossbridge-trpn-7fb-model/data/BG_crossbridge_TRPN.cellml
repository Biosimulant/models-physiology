        <map_variables variable_1="K_U_XB" variable_2="K_U_XB"/>
        <map_variables variable_1="K_W_XB" variable_2="K_W_XB"/>
        <map_variables variable_1="K_S_XB" variable_2="K_S_XB"/>
        <map_variables variable_1="n_Tm" variable_2="n_Tm"/>

        <map_components component_1="K1_MichaelParameters" component_2="environment"/>
        <map_variables variable_1="t" variable_2="t"/>
        <map_variables variable_1="q_K_o" variable_2="q_K_o"/>
        <map_variables variable_1="q_K_i" variable_2="q_K_i"/>
        <map_variables variable_1="q_C_K1" variable_2="q_C_K1"/>
        <map_variables variable_1="q_O_K1" variable_2="q_O_K1"/>
